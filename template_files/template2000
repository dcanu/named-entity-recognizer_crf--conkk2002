# Unigram
U00:%x[-2,0]x
U01:%x[-1,0]x
U02:%x[0,0]x
U03:%x[1,0]x
U04:%x[2,0]x

U05:%x[-1,0]/%x[0,0]x
U06:%x[0,0]/%x[1,0]x

U10:%x[-2,1]x
U11:%x[-1,1]x
U12:%x[0,1]x
U13:%x[1,1]x
U14:%x[2,1]x

U15:%x[-2,1]/%x[-1,1]x
U16:%x[-1,1]/%x[0,1]x
U17:%x[0,1]/%x[1,1]x
U18:%x[1,1]/%x[2,1]x

U20:%x[-2,1]/%x[-1,1]/%x[0,1]
U21:%x[-1,1]/%x[0,1]/%x[1,1]
U22:%x[0,1]/%x[1,1]/%x[2,1]

